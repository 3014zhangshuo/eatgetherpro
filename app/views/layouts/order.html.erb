<!DOCTYPE html>
<html>
    <head>
        <title>Eatgether</title>
        <%= csrf_meta_tags %>

        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
        <script src="http://ogx9q73gu.bkt.clouddn.com/blowup.js" charset="utf-8"></script>
        <link rel="stylesheet" href="http://ogx9q73gu.bkt.clouddn.com/ihover.css" media="screen" title="no title">
    </head>

    <body>

        <div>
            <%= render "common/navbar" %>
            <%= render "common/flashes" %>
            <div class="row">
                <div class="col-md-10 col-md-offset-1 layad">
                    <ul class="nav nav-tabs" role="tablist" >
                        <li  role="presentation" ><%= link_to("发起的预约", account_posts_path) %></li>
                        <li  role="presentation"><%= link_to("申请的预约", account_asker_requests_path) %></li>
                        <li  role="presentation"><%= link_to("约饭订单", account_order_twos_path) %></li>
                    </ul>
                </div>
                <div class="col-md-10 col-md-offset-1">
                    <%= yield %>
                </div>
            </div>
		<%= render "common/footer" %>
    </body>
</html>
