<div class="person-table topnavbar" style="margin-top:80px;">
<div class="row">
    <div class="col-md-8 col-md-offset-2">
			<div><h3>对方信息</h3></div>
        <table class="table table-hover ">
            <tr>
                <th>姓名</th>
                <td><%= @user.nameChi %></td>
            </tr>
            <tr>
                <th>昵称</th>
                <td><%= @user.nameNick %></td>
            </tr>
            <tr>
                <th>性别</th>
                <td><%= @user.gender %></td>
            </tr>
            <tr>
                <th>出生年月</th>
                <td><%= @user.birthday %></td>
            </tr>
            <tr>
                <th>身高</th>
                <td><%= @user.heightUser %></td>
            </tr>
            <tr>
                <th>手机号码</th>
                <td><%= @user.cellNum %></td>
            </tr>
            <tr>
                <th>收入</th>
                <td><%= @user.income %></td>
            </tr>
        </table>
    </div>

</div>
</div>
